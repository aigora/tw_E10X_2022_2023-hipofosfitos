void printList(float vector[], int longitud) 
{
    for (int i = 0; i < longitud; i++) 
    {
        printf("%f ", vector[i]);
    }
    printf("\n");
}


int mayorAmenor(const void* a, const void* b) 
{
    return (*(int*)b - *(int*)a);
}


int menorAmayor(const void* a, const void* b) 
{
    return (*(int*)a - *(int*)b);
}